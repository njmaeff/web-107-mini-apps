{
  "name": "@njmaeff/web-107-node-express-mini-apps",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rm -rf out",
    "dev": "nodemon out/library.js",
    "start": "node out/library.js",
    "sync": "browser-sync start --watch --proxy http://localhost:8080 --reload-debounce 1500 --no-open --files out/pages/** --files public/**",
    "start-db": "docker-compose up -d"
  },
  "dependencies": {
    "@emotion/cache": "^11.6.0",
    "@emotion/react": "^11.7.0",
    "@emotion/server": "^11.4.0",
    "body-parser": "^1.19.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "mongodb": "^4.3.1",
    "mysql": "^2.18.1",
    "polished": "^4.1.4",
    "react": "^17.0.2",
    "react-bootstrap": "^2.1.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/mysql": "^2.15.20",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "browser-sync": "^2.27.7",
    "nodemon": "^2.0.15",
    "typescript": "^4.5.4"
  },
  "packageManager": "yarn@3.2.0"
}
