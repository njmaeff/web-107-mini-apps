{
  "ts-node": {
    "scope": true
  },
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES2015",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "rootDir": "src",
    "outDir": "out",
    "declarationMap": true,
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react"
  },
  "exclude": [
    "public"
  ]
}
